{
    "name": "arafat69/laravel-repository",
    "description": "Laravel Repository Pattern package with built-in Artisan commands. Easily create repositories, follow clean architecture, and simplify database interactions.",
    "keywords": [
        "laravel",
        "laravel package",
        "laravel repository",
        "repository pattern",
        "repository generator",
        "laravel repository pattern",
        "laravel repository generator",
        "artisan command",
        "laravel artisan",
        "laravel code generator",
        "design patterns",
        "laravel design pattern",
        "clean architecture",
        "service repository pattern",
        "php repository pattern"
    ],
    "homepage": "https://github.com/arafat-dev/laravel-repository-old",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Arafat\\LaravelRepository\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Arafat Ali",
            "email": "arafatali.dev@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/support": ">=7.0",
        "illuminate/console": ">=7.0"
    },
    "suggest": {
      "arafatdev/laravel-repository": "Alternative or legacy repository package"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arafat\\LaravelRepository\\RepositoryServiceProvider"
            ],
            "aliases": {
                "Repository": "Arafat\\LaravelRepository\\RepositoryFacade"
            }
        }
    },
  "minimum-stability": "stable",
  "prefer-stable": true
}
